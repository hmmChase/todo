{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "main": "index.js",
  "keywords": [],
  "author": "",
  "license": "",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --exec babel-node ./src/app.js",
    "dev:www": "cross-env NODE_ENV=development nodemon --exec babel-node ./src/bin/www",
    "start": "node ./build/app.js",
    "start:www": "node ./build/bin/www",
    "seed": "node ./src/prisma/seed.js",
    "clean": "rimraf build && mkdir build",
    "build:babel": "babel -d ./build ./src -s --copy-files",
    "build": "npm run clean && npm run build:babel",
    "migrate:dev": "prisma migrate dev --schema=./src/prisma/schema.prisma",
    "migrate:prod": "prisma migrate deploy --schema=./src/prisma/schema.prisma",
    "generate": "prisma generate --schema=./src/prisma/schema.prisma",
    "postinstall": "npm run generate && npm run migrate:prod"
  },
  "dependencies": {
    "@hapi/iron": "^6.0.0",
    "@prisma/client": "^3.6.0",
    "apollo-server-express": "^3.5.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "debug": "^4.3.3",
    "ejs": "^3.1.6",
    "express": "^4.17.2",
    "graphql": "^15.5.3",
    "graphql-middleware": "^6.1.13",
    "graphql-shield": "^7.5.0",
    "graphql-subscriptions": "^2.0.0",
    "helmet": "^4.6.0",
    "http-errors": "^2.0.0",
    "isemail": "^3.2.0",
    "jsonwebtoken": "^8.5.1",
    "juice": "^8.0.0",
    "morgan": "^1.10.0",
    "nodemailer": "^6.7.2",
    "rimraf": "^3.0.2",
    "subscriptions-transport-ws": "^0.11.0",
    "ts-tiny-invariant": "^1.0.5",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.5",
    "@babel/node": "^7.16.5",
    "@babel/plugin-transform-runtime": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "nodemon": "^2.0.15",
    "prisma": "^3.6.0"
  }
}
